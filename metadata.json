{
  "name": "schkovich-mongodb32xenial",
  "version": "0.0.1",
  "author": "Goran Miskovic",
  "summary": "Manage and install MongoDB 3.2.x on Ubuntu 16.04 LTS (Xenial Xerus)",
  "license": "MIT",
  "source": "https://github.com/schkovich/mongodb32xenial",
  "project_page": "https://github.com/schkovich/mongodb32xenial/wiki",
